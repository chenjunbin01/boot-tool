                     <!--handler="com.springboot.tool.core.model.enums.handler.BaseEnumHandler"/>-->
        <!--<typeHandler javaType="com.springboot.tool.core.model.enums.ActivityTypeEnum"-->
                     <!--handler="com.springboot.tool.core.model.enums.handler.BaseEnumHandler"/>-->
    </typeHandlers>

